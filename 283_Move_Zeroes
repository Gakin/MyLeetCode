class Solution(object):
    def moveZeroes(self, nums):
        """
        :type nums: List[int]
        :rtype: void Do not return anything, modify nums in-place instead.
        """
        index = 0
        numberOfZeroes = 0
        while index < len(nums):
            if nums[index] == 0:
                del nums[index]
                numberOfZeroes += 1
            else:
                index += 1
        while numberOfZeroes > 0:
            nums.append(0)
            numberOfZeroes -= 1
